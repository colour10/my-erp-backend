<div id="ell">
<multiple-admin-page v-bind="props" ref="page"></multiple-admin-page>

</div>

<script>
$(document).ready(function(){
    $.post("/common/currency", {}, function(res){
        console.log(res.currency)
        var props = {
            columns:[
                {name:"name", label:"<?=$system_language['guojiamingcheng']?>", is_multiple:true, is_focus:true},
                {name:"code", label:"<?=$system_language['guojiadaima']?>"},                
                {name:"localcurrency", label:"<?=$system_language['bizhong']?>", type:'select', source:"currency"}
            ],
            controller:"country",
            key_column:"name"
        }
        
        new Vue({
            el:"#ell",
            data() {
                return {
                    props:props   
                }
            }
        })
    },"json");
})
</script>
